- var title = "Fun Sport";
doctype html
html
	include includes/head
	body
		.body-wrap
			.container
				header.header 
					.header__logo
						img(src="../public/img/logo.svg", alt="logo")

				main.main
					.main__left
						.main__hero
							picture
								source(srcset='public/img/hero-mob.png' media='(max-width: 768px)')
								img(src="public/img/hero.png", alt="logo")

					.main__right
						.main__right-text 
							.top.green 
								span бонус 200%
							.center 
								sup до 
								span 200 000
								sup.currency ₸
							.bottom
								span НОВЫМ ИГРОКАМ!
						form.form#form
							input(type='hidden' name='country' value='kz')
							.form__item
								.phone_code
									.phone_code__flag
										img#flag_img.phone_code__flag_img(src='public/img/kz.png' alt='kz')
									select#phone_code.phone_code__select(name='')
										option(value='+7-XXX-XXX-XXXX' data-mask='+7-XXX-XXX-XXXX' data-countrycode='KZ') Kazakhstan +7-XXX-XXX-XXXX
								input#tel_desk.com_type(autocomplete='off' type='tel' name='telno' data-mask='com' data-name='tel' placeholder='+7-XXX-XXX-XXXX' data-valid='false' required='')
							.button#formSms
								span Выслать СМС	
							//- .form__item
							//- 	.phone_code
							//- 		.phone_code__flag
							//- 			img(src="public/img/kz.svg" alt="kz")
							//- 		select(name="country" value='kz').phone_code__select#phone_code
							//- 			option(value="kz" data-mask='+7-XXX-XXX-XXXX') Kazakhstan +7-XXX-XXX-XXXX
							//- 	//- .form__input_icon 
							//- 	//- 	.form__country_img
							//- 	//- 		img(src="public/img/kz.svg" alt="kz")
							//- 	//- 	select(name="country" value='kz').form__country_select 
							//- 	//- 		option(value="kz" data-mask='+7-XXX-XXX-XXXX') Kazakhstan +7-XXX-XXX-XXXX
							//- 	input(type="tel" data-valid="false" data-name="phoneNumber" name="phoneNumber" id="phoneNumber" placeholder="+7-XXX-XXX-XXXX").form__input
							//- 	.form__error Wrong phone format
							.form__item
								label.form__select.select
									input(type="hidden" id="crypto" name="currency" data-name="crypto" value="KZT", data-valid='true')
									.select__current
										span KZ (Казахстанский тенге)
									.form__arrow
										img(src="public/img/chevron-down.svg", alt="")
									.select__options
										.option(data-crypto="KZT")
											span KZ (Казахстанский тенге)
							.form__item
								input.input(type='text' placeholder='Введите прмокод (при наличии)' autocomplete='off' name='promo')
							.form__item
								input.input(type='text' placeholder='Код подтверждения' autocomplete='off' name='promo')
							.button#formSms
								span Подтвердить

							.form__item
								label.form__select.select
									input(type="hidden" name="bonus" data-name="sport" value="sport", data-valid='true')
									.select__current
										img(src="public/img/ball.svg", alt="")
										span Бонус на спорт
									.form__arrow
										img(src="public/img/chevron-down.svg", alt="")
									.select__options
										.option(data-bonus="sport")
											img(src="public/img/ball.svg", alt="")
											span Бонус на спорт

							//- .form__item.form_box_r
							//- 	.form_box__rule
							//- 		label(for='check_desk')
							//- 		p Men 18 yoshdan oshganman
							//- 		p.form_box__rule_green Men shartnoma taklifiga rozilik bildiraman
							//- 		input#check_desk(type='checkbox' name='rule' data-name='rule' data-valid='false' required='')
							//- 		label.label_radio(for='check_desk')
								
							.form__item.form__agree 
								span Нажимая кнопку, вы соглашаетесь с 
									a.rules(href="#")  правилами 
									| компании
									br
									| и подтверждаете, что Ваш возраст превышает 21 год

								label.form__agree_label
									input(type="checkbox" name="rule" data-name="rule" checked="checked" data-valid="true")
									.form__agree_checkbox
							
							button.form__button
								span Регистрация

							.form__item.form__item_login
								span У тебя есть аккаунт?  
									a.green(href='#')  Войти

